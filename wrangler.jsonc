{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "premcompanion",
  "main": "src/worker.tsx",
  "compatibility_date": "2025-08-29",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "binding": "ASSETS",
    "directory": "public"
  },
  "observability": {
    "enabled": true
  },
  "d1_databases": [
    {
      "binding": "premcompanion_db",
      "database_name": "premcompanion-db",
      "database_id": "353eef97-22ec-4d1d-9325-88974f7d7bae",
      "migrations_dir": "drizzle",
      "remote": true
    }
  ],
  "r2_buckets": [
    {
      "binding": "R2",
      "bucket_name": "simple-start",
      "preview_bucket_name": "simple-start-dev"
    }
  ]
}
